{
	"name": "TaskChain",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "npx hardhat compile",
		"deploy-testnet": "npx hardhat run ./scripts/MyContract_deploy.js --network ARB_GOERLI",
		"deploy": "npx hardhat run ./scripts/MyContract_deploy.js --network ARB_MAINNET",
		"node": "npx hardhat node",
		"deploy-local": "npx hardhat run ./scripts/MyContract_deploy.js --network localhost"
	},
	"dependencies": {
		"@openzeppelin/contracts": "^4.7.3",
		"dotenv": "^16.4.5"
	},
	"devDependencies": {
		"@nomicfoundation/hardhat-toolbox": "^1.0.2",
		"hardhat": "^2.10.1"
	}
}
